package basic1;
class Student{
	int id;
	String  name;
	double marks;
	String email;
	String dep;
	
	
	
}

public class fristprogramming {

	public static void main(String[] args) {
		Student s1=new Student();
		s1.id=461;
		s1.name="siva";
		s1.marks=88;
		s1.email="19p31a0461@acet.ac.in";
		s1.dep="ECE";
		System.out.println(s1.id+" "+s1.name+" "+s1.marks+" "+s1.email+" "+s1.dep);
		
		

	}

}
